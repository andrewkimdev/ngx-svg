{
  "name": "ngx-svg",
  "version": "1.3.1",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "@svgdotjs/svg.js"
    ],
    "lib": {
      "entryFile": "ngx_svg.ts"
    }
  },
  "description": "Angular Library for creating SVG elements.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "test": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox",
    "test:local": "ng test --watch=true --code-coverage",
    "ng:update": "ng update @angular/core @angular/cli --force",
    "lint": "ng lint",
    "packagr": "ng-packagr -p package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vvaldersteins/ngx-svg.git"
  },
  "keywords": [
    "SVG",
    "Angular",
    "ngx-svg",
    "ng"
  ],
  "author": "Valters Valdersteins",
  "bugs": {
    "url": "https://github.com/vvaldersteins/ngx-svg/issues"
  },
  "homepage": "https://github.com/vvaldersteins/ngx-svg#readme",
  "peerDependencies": {},
  "dependencies": {
    "@svgdotjs/svg.js": "^3.0.16"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/animations": "^12.2.17",
    "@angular/cli": "^12.2.18",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.22",
    "acorn": "^8.0.4",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.0",
    "core-js": "^3.8.3",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.2.7",
    "rxjs": "^6.6.3",
    "ts-node": "^9.1.1",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "zone.js": "~0.11.4"
  }
}